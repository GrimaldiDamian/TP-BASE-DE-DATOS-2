                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with CTE_Total_Facturado AS (
select v.billing_id, v.date, SUM(v.quantity) * SUM(p.PRICE) AS total_facturado
from dbo.temp_ventas v
left join dbo.int_Precios_G01 p
on v.product_id = p.PRODUCT_ID
and v.date &gt;= p.[FROM]
and (v.date &lt; p.UNTIL or p.UNTIL is null)
group by v.billing_id, v.date
)
,
CTE_Descuentos AS (
select v.billing_id, v.date, v.total_facturado, isnull(MAX(d.PERCENTAGE/100.00),0.00) AS descuento
from CTE_Total_Facturado v --1656457
left join dbo.int_Descuento_G01 d
on v.date &gt;= d.[FROM]
and (v.date &lt; d.UNTIL or d.UNTIL is null)
and v.total_facturado &gt; d.TOTAL_BILLING
group by v.billing_id, v.date, v.total_facturado
)
select 
	T.TIEMPO_KEY,
	RE.RANGO_ETARIO_KEY,
	C.CLIENTES_KEY,
	R.REGION_KEY,
	pr.PRODUCTO_KEY,
	E.EMPLEADO_KEY,
	v.BILLING_ID,
	v.REGION,
	v.EMPLOYEE_ID,
	v.CUSTOMER_ID,
	v.PRODUCT_ID,
	v.DATE,
	v.quantity * p.PRICE AS TOTAL,
	v.quantity * p.PRICE * (1-d.descuento)  AS TOTAL_CON_DESCUENTO,
	d.descuento,
	p.PRICE,
	d.descuento AS DESCUENTO,
	case when pr.Envase = 'Lata' then v.quantity else 0 end LATAS_VENDIDAS,
	ROUND(pr.Cantidad * v.quantity, 4) AS LITROS_VENDIDOS
from dbo.temp_ventas v
left join dbo.int_Precios_G01 p
on v.product_id = p.PRODUCT_ID
and v.date &gt;= p.[FROM]
and (v.date &lt; p.UNTIL or p.UNTIL is null)
left join CTE_Descuentos d
on v.billing_id = d.billing_id
left join datawarehouse_2024_G01.dbo.Dim_Productos_G01 pr
on v.Product_ID = pr.Product_ID
LEFT JOIN datawarehouse_2024_G01.dbo.Dim_Clientes_G01 C 
	ON C.CUSTOMER_ID = V.CUSTOMER_ID
LEFT JOIN datawarehouse_2024_G01.dbo.Dim_Empleado_G01 E 
	ON E.EMPLOYEE_ID = V.EMPLOYEE_ID
LEFT JOIN datawarehouse_2024_G01.dbo.Dim_Region_G01 R 
	ON R.[Codigo Postal] = C.ZIPCODE
LEFT JOIN datawarehouse_2024_G01.dbo.Dim_Tiempo_G01 T 
	ON T.FECHA = V.DATE
LEFT JOIN datawarehouse_2024_G01.dbo.Dim_RangoEtario_G01 RE 
	ON DATEDIFF(YEAR, C.BIRTH_DATE, V.[DATE]) 
		BETWEEN RE.EDAD_MINIMA AND RE.EDAD_MAXIMA</property>
